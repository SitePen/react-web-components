{
	"parser": "babel-eslint",
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"rules": {
		"jsx-quotes": ["error", "prefer-double"],
		"react/jsx-uses-react": "error",
		"react/jsx-uses-vars": 2,
		"no-console": 0
	},
	"plugins": [
		"react"
	]
}
